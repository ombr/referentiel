    "http-server": "^0.12.3",
    "jasmine-core": "^4.4.0",
    "jquery": "^3.6.1",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "mathjs": "^11.2.1",
    "pug-cli": "^1.0.0-alpha6",
    "reset-css": "^5.0.1",
