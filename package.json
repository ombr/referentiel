{
  "name": "referentiel",
  "version": "1.0.3",
  "description": "Convert position relative to a html element.",
  "main": "dist/referentiel.js",
  "types": "dist/index.d.ts",
  "module": "dist/referentiel.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
